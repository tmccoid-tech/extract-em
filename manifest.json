{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.6.5",
  "author": "tmccoid.tech",
  "browser_specific_settings": {
    "gecko": {
      "id": "extractem@tmccoid.tech",
      "strict_min_version": "102.0",
      "strict_max_version": "137.0"
    }
  },
  "icons": {
    "64": "icons/extractem-64px.png",
    "32": "icons/extractem-32px.png",
    "16": "icons/extractem-16px.png"
  },
  "background": {
    "page": "background.html"
  },
  "options_ui": {
    "page": "/options/options.html",
    "open_in_tab": false,
    "browser_style": true
  },
  "permissions": [
    "messagesRead",
    "accountsRead",
    "menus",
    "menus.overrideContext",
    "storage",
    "downloads",
    "downloads.open",
    "messagesModifyPermanent"
  ],
  "default_locale": "en",
  "experiment_apis": {
    "deleteAttachmentsApi": {
      "schema": "exp/deleteAttachmentsApi/schema.json",
      "parent": {
        "scopes": [ "addon_parent" ],
        "paths": [ [ "deleteAttachmentsApi" ] ],
        "script": "exp/deleteAttachmentsApi/implementation.js"
      }
    }
  }
}