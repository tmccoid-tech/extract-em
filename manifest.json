{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "2.0.1",
  "author": "tmccoid.tech",
  "browser_specific_settings": {
    "gecko": {
      "id": "extractem@tmccoid.tech",
      "strict_min_version": "128.0"
    }
  },
  "icons": {
    "64": "icons/extractem-64px.png",
    "32": "icons/extractem-32px.png",
    "16": "icons/extractem-16px.png"
  },
  "background": {
    "type": "module",
    "scripts": [ "background.js" ]
  },
  "options_ui": {
    "page": "/options/options.html",
    "open_in_tab": false
  },
  "permissions": [
    "messagesRead",
    "messagesUpdate",
    "accountsRead",
    "menus",
    "menus.overrideContext",
    "storage",
    "downloads",
    "downloads.open",
    "messagesModifyPermanent",
    "messagesTags",
    "messagesTagsList"
  ],
  "default_locale": "en",
  "action": {
    "default_title": " ",
    "default_label": "",
    "default_icon": {
      "64": "icons/extractem-64px.png",
      "32": "icons/extractem-32px.png",
      "16": "icons/extractem-16px.png"
    }
  },  
  "message_display_action": {
    "default_title": " ",
    "default_label": "",
    "default_icon": {
      "64": "icons/extractem-64px.png",
      "32": "icons/extractem-32px.png",
      "16": "icons/extractem-16px.png"
    }
  },
  "experiment_apis": {
    "ExtractionFilterAction": {
      "schema": "api/ExtractionFilterAction/schema.json",
      "parent": {
        "scopes": [ "addon_parent" ],
        "paths": [ [ "ExtractionFilterAction" ] ],
        "script": "api/ExtractionFilterAction/implementation.js"
      }
    }
  }
}